<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.furniturestore.controllers.store.InformationController">
   <center>
      <VBox fx:id="allContainer">
         <HBox fx:id="labelsContainerHBox" styleClass="vBox-container" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
               <VBox fx:id="namesContainerVBox" prefHeight="200.0" prefWidth="100.0" />
               <VBox fx:id="valueContainerVBox" prefHeight="200.0" prefWidth="100.0" />
            </children>
         </HBox>
         <HBox fx:id="buttonsContainerHBox" prefHeight="100.0" prefWidth="200.0">
            <children>
               <Button onAction="#onBackButton" text="Back" />
            </children>
         </HBox>
      </VBox>
   </center>
</BorderPane>
